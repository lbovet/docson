{
  "name": "docson",
  "version": "2.1.0",
  "description": "Documentation for your JSON types",
  "main": "lib/index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "bin": "./lib/docson-cli.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run build-lib && npm run build-widget",
    "build-widget": "webpack",
    "build-lib": "babel src -d lib",
    "clean": "rm -fr lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lbovet/docson"
  },
  "keywords": [
    "json",
    "schema",
    "documentation"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "author": "Laurent Bovet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lbovet/docson/issues"
  },
  "homepage": "https://github.com/lbovet/docson#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "jest": "^23.2.0",
    "jest-environment-jsdom": "^23.4.0",
    "puppeteer": "^1.5.0",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "jquery": "^2.2.4",
    "jsonpointer": "^4.0.1",
    "jsonpointer.js": "^0.4.0",
    "lodash": "^4.17.10",
    "marked": "^0.3.14",
    "serve-index": "^1.9.1",
    "traverse": "^0.6.6",
    "urijs": "^1.19.1"
  }
}
